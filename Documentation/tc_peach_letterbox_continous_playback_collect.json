{
  "peach_schema_version": "1.0.0",
  "peach_implementation_version": "0.9.6",
  "site_key": "#peach_site_key#",
  "session_start_timestamp": #session_start_timestamp#,
  "sent_timestamp": #sent_timestamp#,
  "user_id": "#user_id#",
  "client": {
    "id": "#client_id#",
    "type": "mobileapp",
    "app_id": "#client_app_id#",
    "name": "#client_name#",
    "version": "#client_version#",
    "device": {
      "type": "#client_device_type#",
      "vendor": "#client_device_vendor#",
      "model": "#client_device_model#",
      "screen_size": "#client_device_screen_size#",
      "language": "#client_device_language#"
    },
    "os": {
      "name": "#client_os_name#",
      "version": "#client_os_version#"
    }
  },
  "events": [
    {
      "type": "#event_type#",
      "id": "#event_id#",
      "event_timestamp": #event_timestamp#,
      "context": {
        "items" : [ "#event_context_items_urn#" ],
        "items_displayed" : 1,
        "hit_index" : #event_context_hit_index#,
        "page_uri" : "#event_context_page_uri#",
        "action" : "#event_context_action#",
        "component" : {
          "type": "Player page",
          "name": "#event_context_component_name#",
          "version": "#event_context_component_version#",
          "feature": "continuous_playback"
        }
      }
    }
  ]
}