{
  "peach_schema_version": "1.0.0",               
  "peach_implementation_version":"0.0.1",        
  "site_key": "#peach_site_key#",         
  "session_start_timestamp": "#session_start_timestamp#",            
  "sent_timestamp": "#sent_timestamp#",
  "client": {                                    
    "id" : "#client_id#",                                                                         
    "type" : "mobileapp",                                        
    "app_id" : "#client_app_id#",            
    "name" : "#client_name#",                         
    "version": "#client_version#",                                                             
    "device" :{                                  
      "type":"#client_device_type#",                            
      "vendor": "#client_device_vendor#",                         
      "model": "#client_device_model#",                     
      "screen_size": "#client_device_screen_size#",                
      "language": "#client_device_language#"
    },                                                                                        
    "os" :{                                      
      "name": "#client_os_name#",                             
      "version": "#client_os_version#"            
    } 
  },
  "events" : [                                   
    {                                            
      "type": "#event_type#",                      
      "id": "#event_id#",                           
      "event_timestamp": "#event_timestamp#",
      "props" : {                                
        "playback_position_s": "#event_props_playback_position_s#",
        "playback_timeshift_s": "#event_props_playback_timeshift_s#",
        "playback_rate": "1.0"
      },                
      "context" : {
        "page_uri" : "#page_uri#",                             
        "component" : {                          
          "type": "player",                      
          "name": "#event_context_component_name#",                   
          "version": "#event_context_component_version#"        
        }
      },
      "metadata" : {                                                                      
        "type": "#event_context_metadata_type#",
        "format": "#event_context_metadata_format#",
        "episode_id": "#event_context_metadata_episode_id#",                  
        "show_id": "#event_context_metadata_show_id#", 
        "producer": "#event_context_metadata_producer#",
        "duration": "#event_context_metadata_duration#"
      }
    }
  ]      
}